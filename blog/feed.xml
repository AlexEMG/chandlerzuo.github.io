---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Lanfeng Blog</title>
 <link href="http://panlanfeng.github.com/blog/feed.xml" rel="self"/>
 <link href="http://panlanfeng.github.com/blog"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://panlanfeng.github.com/blog</id>
 <author>
   <name>Lanfeng Pan</name>
   <email>panlanfeng#gmail.com</email>
 </author>

 {% for post in site.categories.blog %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://panlanfeng.github.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://panlanfeng.github.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
